<style lang="less">
    @import "lotusNoData.less";
</style>
<template>
    <view class="lotus-nodata {{isNofixed ? 'lotus-nodata-nofixed' : ''}}" style="height:{{height}}%" wx:if="{{show}}">
        <view class="lotus-nodata-pic">
            <image class="pic" src="{{imageUrl}}" mode="widthFix"></image>
        </view>
        <view class="lotus-nodata-des" wx:if="{{showTxt}}">
            <view wx:for="{{textArr}}" wx:key="index" index="index" item="item">{{item}}</view>
        </view>
        <slot name="condition"></slot>
        <view class="lotus-nodata-btn" wx:if="{{showBtn}}" bindtap="noDataClick">{{btnTxt}}</view>
    </view>
</template>
<script>
    import wepy from 'wepy'

    export default class NoData extends wepy.component {
      data = {
        show: false,
        showBtn: false,
        showTxt: true,
        textArr: ['暂无数据'],
        imageUrl: 'data:image/jpeg;base64,',
        imgType: 0, // 0:默认/无订单（可不传）1:无地址,2:无收藏,3:无商品,4:无消息,5:空采购车,6:无优惠券
        btnTxt: '返回',
        isNofixed: false
      };
      props={
        height: {
          default: 100,
          type: Number
        }
      };
        // 方法调用
        // 方法*
      methods = {
            // 搜索
        noDataClick() {
          this.$emit('noDataClick')
        }
      };
        // 显示
      init(data) {
        if (data) {
          for (let attr in data) {
            this[attr] = data[attr]
          }
          if (data.imgType) {
            // const imgUrl = 'https://config.xiaoyaozhan.com/images/myb/noData/'
            switch (data.imgType) {
              case 1:// 无地址
                this.imageUrl = 'data:image/jpeg;base64,'
                break
              case 2:// 无收藏
                this.imageUrl = 'data:image/jpeg;base64,'
                break
              case 3:// 无商品
                this.imageUrl = 'data:image/jpeg;base64,'
                break
              case 4:// 无消息
                this.imageUrl = 'data:image/jpeg;base64,'
                break
              case 5:// 空采购车
                this.imageUrl = 'https://tse1-mm.cn.bing.net/th/id/OIP.QB4KLvCGudnLDeUqPB9AXQHaFj?w=230&h=180&c=7&o=5&pid=1.7'
                break
              case 6:// 无优惠券
                this.imageUrl = 'https://tse3-mm.cn.bing.net/th/id/OIP.QirpDFcnKIYlFh8B_QNZ5QHaE8?w=259&h=180&c=7&o=5&pid=1.7'
                break
            }
          }
          this.$apply()
        } else {
          this.show = false
          this.$apply()
        }
      };
      onLoad() {
      }
    }
</script>
